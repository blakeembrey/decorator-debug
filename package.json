    "url": "http://blakeembrey.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blakeembrey/decorator-debug/issues"
  },
  "homepage": "https://github.com/blakeembrey/decorator-debug",
  "devDependencies": {
    "chai": "^3.2.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  },
  "dependencies": {
    "performance-now": "^0.2.0"
